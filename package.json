    "deploy": "gh-pages -d src"
  },
  "dependencies": {
    "gh-pages": "^3.2.3",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.18"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
